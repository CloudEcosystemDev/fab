{
  "name": "@fab/cli",
  "version": "0.0.5-alpha.3",
  "description": "The CLI entry-point for the FAB ecosystem",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/fab-spec/fab",
  "bugs": "https://github.com/fab-spec/fab/issues",
  "repository": "fab-spec/fab",
  "license": "MIT",
  "author": "Glen Maddern @geelen",
  "files": [
    "/bin",
    "/lib",
    "/oclif.manifest.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "fab": "./bin/run"
  },
  "scripts": {
    "build": "yarn build:cjs",
    "build:cjs": "tsc --module commonjs --outDir lib",
    "build:watch": "yarn build:cjs --watch --preserveWatchOutput",
    "clean": "rm -rf lib",
    "coverage": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "oclif:generate": "oclif-dev manifest && oclif-dev readme",
    "prepack": "npm run clean && npm run build && npm run oclif:generate",
    "postpack": "rm -f oclif.manifest.json",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@fab/core": "0.0.5-alpha.3",
    "@fab/server": "0.0.5-alpha.3",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/jju": "^1.4.1",
    "@types/node": "^10",
    "@types/prettier": "^1.19.0",
    "@types/semver": "^6.2.0",
    "chalk": "^3.0.0",
    "deterministic-zip": "^1.1.0",
    "execa": "^4.0.0",
    "fs-extra": "^8.1.0",
    "jju": "^1.4.0",
    "path-browserify": "^1.0.0",
    "pkg-up": "^3.1.0",
    "prettier": "^1.19.1",
    "regex-parser": "^2.2.10",
    "rollup": "^1.27.10",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-hypothetical": "^2.1.0",
    "semver": "^7.1.1",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "98ce34423f906c0a9f66f2f87643e64b841e359a",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fab",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
