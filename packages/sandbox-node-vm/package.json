{
  "name": "@fab/sandbox-node-vm",
  "version": "1.0.0-rc.2.alpha.1",
  "description": "FAB runtime sandbox using Node's 'vm'",
  "keywords": [
    "fab",
    "server"
  ],
  "homepage": "https://github.com/fab-spec/fab#readme",
  "bugs": {
    "url": "https://github.com/fab-spec/fab/issues"
  },
  "repository": "fab-spec/fab",
  "license": "MIT",
  "author": "Glen Maddern",
  "files": [
    "lib",
    "esm"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir lib",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:watch": "run-p 'build:{cjs,esm} -- --watch --preserveWatchOutput'",
    "clean": "rm -rf esm lib package-lock.json",
    "prepack": "npm run clean && npm run build"
  },
  "dependencies": {
    "@fab/core": "1.0.0-rc.2.alpha.1",
    "node-fetch": "^2.6.0",
    "web-streams-ponyfill": "^1.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "7993c1665c36c74ab0e8b4bcf0efb43c80490f65"
}
