{
  "name": "@fab/actions",
  "version": "0.1.3",
  "private": false,
  "description": "The guts of the FAB cli code, keeping the 'fab' package lean",
  "keywords": [
    "build",
    "cli",
    "compile",
    "deploy",
    "fab"
  ],
  "homepage": "https://github.com/fab-spec/fab",
  "repository": "fab-spec/fab",
  "license": "MIT",
  "author": "Glen Maddern",
  "files": [
    "lib",
    "esm"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir lib",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:watch": "run-p 'build:{cjs,esm} -- --watch --preserveWatchOutput'",
    "clean": "rm -rf esm lib package-lock.json",
    "prepack": "npm run clean && npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@fab/core": "0.1.3",
    "@fab/cli": "0.1.3",
    "@rollup/plugin-alias": "^2.2.0",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-inject": "^4.0.1",
    "@rollup/plugin-json": "^4.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "@rollup/plugin-typescript": "^2.1.0",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.12.14",
    "@types/webpack": "^4.41.5",
    "buffer": "^5.4.3",
    "deterministic-zip": "^1.1.0",
    "fs-extra": "^8.1.0",
    "memfs": "^3.0.4",
    "path-browserify": "^1.0.0",
    "rollup": "^1.27.10",
    "rollup-plugin-hypothetical": "^2.1.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "util": "^0.12.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "98ce34423f906c0a9f66f2f87643e64b841e359a"
}
