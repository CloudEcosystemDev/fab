{
  "name": "@fab/input-static",
  "version": "0.0.3-alpha.0+98ce344",
  "description": "Module to handle files outside _assets",
  "keywords": [
    "fab"
  ],
  "homepage": "https://github.com/fab-spec/fab#readme",
  "bugs": {
    "url": "https://github.com/fab-spec/fab/issues"
  },
  "repository": "fab-spec/fab",
  "license": "MIT",
  "author": "Glen Maddern",
  "files": [
    "cjs",
    "esm",
    "src"
  ],
  "type": "module",
  "exports": {
    "default": "./esm/index.js",
    "require": "./cjs/index.js"
  },
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:watch": "yarn build:cjs --watch --preserveWatchOutput",
    "clean": "rm -rf esm cjs",
    "prepack": "npm run clean && npm run build",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\""
  },
  "dependencies": {
    "@fab/core": "^0.0.3-alpha.0+98ce344",
    "@types/node": "^12.12.14",
    "fs-extra": "^8.1.0",
    "globby": "^10"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "98ce34423f906c0a9f66f2f87643e64b841e359a"
}
