{
  "name": "@fab/nextjs",
  "description": "FAB compiler for NextJS projects",
  "version": "0.4.0",
  "author": "Glen Maddern @geelen",
  "bin": {
    "fab-nextjs": "./bin/run"
  },
  "bugs": "https://github.com/fab-spec/fab/issues",
  "dependencies": {
    "@fab/compile": "0.3.1",
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^5.0.4",
    "chalk": "^2.4.1",
    "fs-extra": "^7.0.1",
    "globby": "^10",
    "memfs": "2.12.1",
    "mock-express-response": "^0.2.2",
    "prettier": "^1.16.4",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "typescript": "^3.7.2",
    "@types/node": "^12.12.14",
    "chai": "^4",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/fab-spec/fab",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fab-nextjs",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "fab-spec/fab",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "clean": "rm -rf lib",
    "prepack": "yarn clean && tsc -d && cp -R src/files lib/files && oclif-dev manifest && oclif-dev readme",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
