{
  "name": "docs",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build:fab": "npm run docz:build && npm run fab:build",
    "docz:build": "node scripts/build.js && docz build",
    "docz:dev": "concurrently \"docz dev --port=2999\" \"node scripts/watch.js\"",
    "docz:serve": "docz build && docz serve",
    "fab:build": "fab build",
    "fab:serve": "fab serve fab.zip",
    "fab:deploy": "dotenv -- fab deploy fab.zip --assets-already-deployed-at='https://static-assets.linc.sh/fab-docs'"
  },
  "devDependencies": {
    "@fab/cli": "^0.0.7-alpha.30",
    "@fab/input-static": "^0.0.7-alpha.30",
    "@fab/rewire-assets": "^0.0.7-alpha.30",
    "@fab/serve-html": "^0.0.7-alpha.30",
    "@fab/server": "^0.0.7-alpha.30",
    "chokidar": "^3.3.1",
    "concurrently": "^5.0.2",
    "docz": "^2.2.0",
    "fast-glob": "^3.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "^1.0.4"
  }
}
